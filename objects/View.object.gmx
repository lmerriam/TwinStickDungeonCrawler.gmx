          <argument>
            <kind>1</kind>
            <string>/// Move 10% toward player and screen shake

if (instance_exists(Player)) {
    var dir = point_direction(Player.x,Player.y,mouse_x,mouse_y);
    var dis = point_distance(Player.x,Player.y,mouse_x,mouse_y);
    var targetx = Player.x + lengthdir_x(dis/10,dir);
    var targety = Player.y + lengthdir_y(dis/10,dir);

    x += (targetx - x) * .1  * (30/room_speed);
    y += (targety - y) * .1 * (30/room_speed);
}

if (global.screen_shake &gt; 4) {
    if (global.screen_shake &gt; 6) global.screen_shake = 8;
    x += choose(-random(global.screen_shake),random(global.screen_shake)) * (30/room_speed);
    y += choose(-random(global.screen_shake),random(global.screen_shake)) * (30/room_speed);
}
</string>
          </argument>
        </arguments>
      </action>
