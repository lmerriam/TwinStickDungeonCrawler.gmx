            <string>/// Move 10% toward player and screen shake

if (instance_exists(Player)) {
    var dir = point_direction(Player.x,Player.y,mouse_x,mouse_y);
    var dis = point_distance(Player.x,Player.y,mouse_x,mouse_y);
    var targetx = Player.x + lengthdir_x(dis/10,dir);
    var targety = Player.y + lengthdir_y(dis/10,dir);

