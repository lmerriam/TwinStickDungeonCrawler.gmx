view_xview[1] = Player.x - view_wview[1]/2;
view_yview[1] = Player.y - view_hview[1]/2;

if (!global.cast_mode) {
    // Check for nearest drop
    nearest_drop = instance_nearest(x,y,Drop_Parent);
