// Init cast mode
global.cast_mode = false;
cast_btn_width = 96;
cast_btn_x1 = window_width - cast_btn_width - windowpadding;
cast_btn_y1 = window_height - windowpadding - cast_btn_width;
cast_btn_x2 = cast_btn_x1 + cast_btn_width;
cast_btn_y2 = cast_btn_y1 + cast_btn_width;

// Init minimap
minimap_width = window_height/5;
minimap_height = minimap_width;
minimap_window = 500;
minimap_window_x = window_width - minimap_width - windowpadding;
minimap_window_y = windowpadding;
minimap = -1;

// Init menu button
var width = .1 * window_width;
var height = width;
menu_btn_origin_x = windowpadding + width/2;
menu_btn_origin_y = windowpadding + height/2;
menu_btn_x1 = windowpadding;
menu_btn_x2 = menu_btn_x1+width;
menu_btn_y1 = windowpadding;
menu_btn_y2 = menu_btn_y1 + height;

// Init nearest drop UI
nearest_drop = noone;
nearest_drop_in_range = false;
nearest_button_height = 40;

nearest_button_x1 = 0;
nearest_button_x2 = 0;
nearest_button_y1 = 0;
nearest_button_y2 = 0;
</string>
