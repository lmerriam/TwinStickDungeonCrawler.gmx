        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Open menu
var xx = device_mouse_x_to_gui(0);
var yy = device_mouse_y_to_gui(0);

if (global.cast_mode) {
    var spell = scr_get_spell().script;
    script_execute(spell);
    global.cast_mode = false;
    global.timescale = 1;
} else {
    // Detect click on cast mode butn
    if (scr_mouse_over_ui(cast_btn_x1,cast_btn_y1,cast_btn_x2,cast_btn_y2) and Player.alarm[1] &lt;= 0 and Player.stamina &gt;= scr_get_spell().fatigue) {
        global.cast_mode = true;
        global.timescale = .1;
        
    // Detect click on menu btn
    } else if (xx &gt; menu_btn_x1 and xx &lt; menu_btn_x2 and yy &gt; menu_btn_y1 and yy &lt; menu_btn_y2) {
        with(View) {
            event_perform(ev_keypress,ord('I'));
        }
    
    } else if (scr_mouse_over_ui(minimap_window_x,minimap_window_y,minimap_window_x+minimap_width,minimap_window_y+minimap_height)) {
        with(View) {
            event_perform(ev_keypress,ord('M'));
        }
        
    // Detect pickup button click
    } else if (nearest_drop != noone and scr_mouse_over_ui(nearest_button_x1,nearest_button_y1,nearest_button_x2,nearest_button_y2)) {
        scr_send_to_inv(nearest_drop);
    }
}
</string>
