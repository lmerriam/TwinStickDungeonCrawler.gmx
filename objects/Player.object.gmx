        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Control stats
scr_calc_stats();

// HP
if (hp &lt; 0) {
    hp = 0;
}

// Stamina regen
if (stamina &lt; maxstamina and scr_alarm_passed(stamina_timer)) {
    stamina++;
    stamina_timer = scr_sec_from_now(1);
}
