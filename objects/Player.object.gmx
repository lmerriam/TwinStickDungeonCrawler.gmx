        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Go through door

if (room_exists(other.new_room)) {

    // Activate objects that need to persist
    // They will automatically be deactivated again by the view after they've transitioned to the new room
    for (column = 0; column &lt; Inventory.grid_width; column++) {
        for (row = 0; row &lt; Inventory.grid_height; row++) {
            instance_activate_object(Inventory.inventory_slots[# column,row]);
        }
    }
    instance_activate_object(global.current_weapon);
    instance_activate_object(global.current_spell);
    instance_activate_object(global.current_armor);
    instance_activate_object(global.current_gem);
    
    room_goto(other.new_room);
    x = other.new_x;
    y = other.new_y;
    View.x = other.new_x;
    View.y = other.new_y;
}
</string>
