            <string>/// Collect expr
with (other) instance_destroy();

// Level up
expr += 1;
if (expr &gt;= maxexpr) {
    level += 1;
    expr = expr-maxexpr;
    maxexpr += level;
    base_maxhp += 2;
    hp = maxhp;
    base_maxstamina += 2;
    stamina = maxstamina;
    scr_announce("Achieved level " + string(level), noone);
}


